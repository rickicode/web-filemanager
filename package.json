{
  "name": "web-file-manager",
  "version": "1.0.0",
  "description": "Simple web file manager with Node.js and Docker",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "start:host": "HOST=0.0.0.0 PORT=3000 node app.js",
    "start:public": "HOST=0.0.0.0 PORT=3000 ADMIN_USERNAME=admin ADMIN_PASSWORD=admin123 node app.js"
  },
  "keywords": [
    "file-manager",
    "nodejs",
    "docker",
    "express"
  ],
  "author": "Web File Manager",
  "license": "MIT",
  "dependencies": {
    "archiver": "^5.3.2",
    "connect-redis": "^8.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.20.0",
    "express-session": "^1.18.1",
    "fs-extra": "^11.3.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.0-rc.4",
    "path": "^0.12.7",
    "redis": "^5.5.6"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
